<UserControl x:Class="AP8PO.UserControls.ManageCommitsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AP8PO.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Button Click="GenerateCommits_ButtonClick">Generate commits</Button>
            <DataGrid x:Name="list" ItemsSource="{Binding CourseCommits}" IsReadOnly="True">
                
            </DataGrid>
        </StackPanel>
    </Grid>
</UserControl>
